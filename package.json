{
    "name": "elixir-zigler",
    "displayName": "elixir-zigler",
    "description": "Highlight ~Z as Zig in Elixir",
    "version": "0.1.0",
    "repository": {
        "url": "https://github.com/Kabie/vscode-elixir-zigler"
    },
    "engines": {
        "vscode": "^1.69.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "grammars": [{
            "scopeName": "source.elixir.zigler",
            "path": "./syntaxes/elixir-zigler.injection.json",
            "injectTo": ["source.elixir"],
            "embeddedLanguages": {
                "meta.embedded.block.zig": "zig"
            }
        }]
    }
}
